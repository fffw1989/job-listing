<%= simple_form_for [:admin, @job ] do |f| %>

<%= f.input :title, :label =>"服务" %>
<%= f.input :description, :label => "服务内容" %>
<%= f.input :wage_lower_bound, :label => "服务时长（小时）" %>
<%= f.input :wage_upper_bound, :label => "服务佣金（元）" %>
<%= f.input :contact_email, :label => "联系方式" %>
<%= f.input :is_hidden, :label => "隐藏" %>
  <%= f.submit "确定" %>
<% end %>
